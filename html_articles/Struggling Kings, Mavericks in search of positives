			</script>
				
	<!-- SiteCatalyst code version: AppMeasurement 1.0 Copyright 1996-2013 Adobe, Inc. -->
	<script type="text/javascript">

	if (typeof s_omni === 'undefined') {
		jQuery(function($){
			$.getScriptCache("http://a.espncdn.com/redesign/0.387.2/js/espn-analytics.js", 				function() {					var deferEvent = "user.parsed",						initOnLoad = null || false,						timeoutID;
					function initTrack(){						if (typeof espn.track.init === 'function') {							window.clearTimeout(timeoutID);														$.unsubscribe([deferEvent, initTrack]);
							espn.track.init(data);						}					}										if(initOnLoad || window.espn_ui.userParsed){						initTrack();					}					else {						$.subscribe(deferEvent, initTrack);                        timeoutID = window.setTimeout(initTrack, 5000);					}				}				);
		});
	}

	</script>

	<!-- End SiteCatalyst code version: AppMeasurement 1.0 -->

        <script>
           var espn = espn || {};
           espn.ads = espn.ads || {};
           var siteIds = {
                "espn-en": 24667,
                // espnw: 24767,
                "espnuk-en": 24765,
                // rugby: 24775,
                // f1: 24757,
                "espncricinfo-en-us": 24753,
                "espnau-en": 42938,
                "espn-es-us": 24755,
                "espnza-en": 45599,
                "espn-es-ar": 42934,
                "espn-es-ve": 42944,
                "espn-es-cl": 42932,
                "espn-es-co": 42940,
                "espn-es-mx": 42942
            };
            var site = espn.i18n.editionKey ? espn.i18n.editionKey : "espn-en";
            siteId = siteIds[site];
            if (siteId) {
                var bk_iframe = document.createElement('iframe');
                bk_iframe.style.display = "none";
                bk_iframe.src="https://tags.bluekai.com/site/" + siteId + "?ret=html";
                document.body.appendChild(bk_iframe);
            }

            window.addEventListener("message", receiveMessage, false);

            function receiveMessage (event) {
                if (/tags\.bluekai/.test(event.origin)) {
                    if (espn.ads.config && espn.ads.config.kvps) {
                        espn.ads.config.kvps.push({name:"espn_yxbk", value:event.data});
                    }
                }
            }
        </script>
	</body>
</html>